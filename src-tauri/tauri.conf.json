{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "https://discord.com/app",
    "distDir": "https://discord.com/app",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "soundstyle",
    "version": "0.0.5"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.soundstyle.dev",
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "soundstyle",
        "width": 800,
        "height": 600,
        "transparent": false,
        "decorations": true
      }
    ]
  }
}
